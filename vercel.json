{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://localeffortfood.com https://*.localeffortfood.com http://localhost:*"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://localeffortfood.com/tiny-weddings",
      "has": [
        {
          "type": "host",
          "value": "tiny-wedding-1.vercel.app"
        }
      ],
      "permanent": true
    }
  ]
}
